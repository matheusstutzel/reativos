code/tight Intersects (var& SDL_Rect r1, var& SDL_Rect r2) -> bool
do
    escape  not((r1.x > r2.x+r2.w) or (r1.x+r1.w < r2.x) or (r1.y > r2.y+r2.h) or (r1.y+r1.h < r2.y));
end
